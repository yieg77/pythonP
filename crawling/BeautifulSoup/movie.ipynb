       "\n",
       "            var CurCookieName = 'CgvPopAd-' + ArrAdUrl[3];\n",
       "            var CurCookieUrl = GetCookieAd(CurCookieName);\n",
       "            var CookieUrl = ArrAdUrl[3];\n",
       "\n",
       "            CookieUrl = EncryptAD(CookieUrl, \"15442280\");\n",
       "            setCookieAD(CurCookieName, CookieUrl, '1');\n",
